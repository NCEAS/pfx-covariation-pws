	keywords = {Biocomplexity, Chinook, coherence, portfolio effect, response diversity},
	pages = {340--348},
	file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/QUFU9K62/Moore et al. - 2010 - Synchronization and portfolio performance of threa.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/G4UDZHPB/abstract.html:text/html}
