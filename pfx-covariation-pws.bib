@article{anderson_community_1999,
	title = {Community reorganization in the {Gulf} of {Alaska} following ocean climate regime shift},
	volume = {189},
	url = {http://www.int-res.com/abstracts/meps/v189/p117-123/},
	doi = {10.3354/meps189117},
	abstract = {ABSTRACT: A shift in ocean climate during the late 1970s triggered a reorganization of community structure in the Gulf of Alaska ecosystem, as evidenced in changing catch composition on long-term (1953 to 1997) small-mesh trawl surveys. Forage species such as pandalid shrimp and capelin declined because of recruitment failure and predation, and populations have not yet recovered. Total trawl catch biomass declined {\textgreater}50\% and remained low through the 1980s. In contrast, recruitment of high trophic-level groundfish improved during the 1980s, yielding a {\textgreater}250\% increase in catch biomass during the 1990s. This trophic reorganization apparently had negative effects on piscivorous sea birds and marine mammals.},
	urldate = {2016-10-26},
	journal = {Marine Ecology Progress Series},
	author = {Anderson, Paul J. and Piatt, John F.},
	month = nov,
